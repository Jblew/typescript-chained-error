{
  "name": "universe-log",
  "version": "0.1.0",
  "description": "Universal javascript log for node and browser. Suitable for libraries and microservices. Configurable via env and window. Extendable",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.15"
  },
  "scripts": {
